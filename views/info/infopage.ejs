<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="/css/product.css">
</head>

<body>
<%- include('../includes/navigation.ejs') %>

<main>
    <% if (processInfo) { %>
        <div>
            <h1>Process Args </h1>
            <% for (let processArg of processInfo.processArgs) { %>
                <div>
                    <p><%= processArg %></p>
                </div>
            <% } %>
            <p>Process Platform: <%= processInfo.processPlatform %> </p>
            <p>Node.Js Version: <%= processInfo.processVersion %> </p>
            <p>RSS Memory Usage: <%= processInfo.processMemoryUsage %> </p>
            <p>Execution Path: <%= processInfo.processExecutionPath %> </p>
            <p>Process ID: <%= processInfo.processId %></p>
            <p>Project Folder: <%= processInfo.processCwd %></p>
        </div>
    <% } else { %>
        <h1>No Process Info Found!</h1>
    <% } %>
</main>
<%- include('../includes/end.ejs') %>